           0CA : 10E3;  -- 	STORE  dres16sR
           0CB : B801;  -- 	LOADI  1
           0CC : 10E0;  -- 	STORE  d16sC1       ; set carry
           0CD : 28B9;  -- 	JUMP   Div16s_loop
