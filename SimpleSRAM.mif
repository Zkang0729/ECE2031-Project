           053 : 10A6;  -- 	STORE  m16sB
           054 : 8060;  -- 	CALL   A2_mult      ; convert to degrees
           055 : 107F;  -- 	STORE  AtanT
           056 : 6017;  -- 	SHIFT  -7           ; check 7th bit
           057 : 491C;  -- 	AND    One
           058 : 405D;  -- 	JZERO  A2_rdwn      ; round down
           059 : 087F;  -- 	LOAD   AtanT
           05A : 6018;  -- 	SHIFT  -8
           05B : 6801;  -- 	ADDI   1            ; round up
           05C : 8800;  -- 	RETURN
           05D : 087F;  -- 	LOAD   AtanT
           05E : 6018;  -- 	SHIFT  -8           ; round down
           05F : 8800;  -- 	RETURN
