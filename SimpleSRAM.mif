           073 : 58DC;  -- 	XOR    d16sN        ; if the sign changed,
           074 : 3078;  -- 	JNEG   A2_DD        ; switch to scaling the denominator
           075 : 58DC;  -- 	XOR    d16sN        ; get back shifted version
           076 : 10DC;  -- 	STORE  d16sN
           077 : 286B;  -- 	JUMP   A2_DL
           078 : 08DD;  -- 	LOAD   d16sD
           079 : 6011;  -- 	SHIFT  -1           ; have to scale denominator
           07A : 10DD;  -- 	STORE  d16sD
           07B : 286B;  -- 	JUMP   A2_DL
           07C : 0000;  -- AtanX:      DW 0
           07D : 0000;  -- AtanY:      DW 0
           07E : 0000;  -- AtanRatio:  DW 0        ; =y/x
           07F : 0000;  -- AtanT:      DW 0        ; temporary value
           080 : 0048;  -- A2c:        DW 72       ; 72/256=0.28125, with 8 fractional bits
           081 : 394C;  -- A2cd:       DW 14668    ; = 180/pi with 8 fractional bits
           082 : B800;  -- 	LOADI  0
           083 : 10A7;  -- 	STORE  m16sc        ; clear carry
           084 : 10AA;  -- 	STORE  mres16sH     ; clear result
           085 : B810;  -- 	LOADI  16           ; load 16 to counter
           086 : 10A8;  -- 	STORE  mcnt16s      
           087 : 08A7;  -- 	LOAD   m16sc        ; check the carry (from previous iteration)
           088 : 408C;  -- 	JZERO  Mult16s_noc  ; if no carry, move on
           089 : 08AA;  -- 	LOAD   mres16sH     ; if a carry, 
           08A : 18A5;  -- 	ADD    m16sA        ;  add multiplicand to result H
           08B : 10AA;  -- 	STORE  mres16sH
           08C : 08A6;  -- 	LOAD   m16sB
           08D : 491C;  -- 	AND    One          ; check bit 0 of multiplier
           08E : 10A7;  -- 	STORE  m16sc        ; save as next carry
           08F : 4093;  -- 	JZERO  Mult16s_sh   ; if no carry, move on to shift
           090 : 08AA;  -- 	LOAD   mres16sH     ; if bit 0 set,
           091 : 20A5;  -- 	SUB    m16sA        ;  subtract multiplicand from result H
           092 : 10AA;  -- 	STORE  mres16sH
           093 : 08A6;  -- 	LOAD   m16sB
           094 : 6011;  -- 	SHIFT  -1           ; shift result L >>1
           095 : 48A4;  -- 	AND    c7FFF        ; clear msb
           096 : 10A6;  -- 	STORE  m16sB
           097 : 08AA;  -- 	LOAD   mres16sH     ; load result H
           098 : 600F;  -- 	SHIFT  15           ; move lsb to msb
           099 : 50A6;  -- 	OR     m16sB
           09A : 10A6;  -- 	STORE  m16sB        ; result L now includes carry out from H
           09B : 08AA;  -- 	LOAD   mres16sH
           09C : 6011;  -- 	SHIFT  -1
           09D : 10AA;  -- 	STORE  mres16sH     ; shift result H >>1
           09E : 08A8;  -- 	LOAD   mcnt16s
           09F : 6FFF;  -- 	ADDI   -1           ; check counter
           0A0 : 3886;  -- 	JPOS   Mult16s_loop ; need to iterate 16 times
           0A1 : 08A6;  -- 	LOAD   m16sB
           0A2 : 10A9;  -- 	STORE  mres16sL     ; multiplier and result L shared a word
           0A3 : 8800;  -- 	RETURN              ; Done
           0A4 : 7FFF;  -- c7FFF: DW &H7FFF
           0A5 : 0000;  -- m16sA: DW 0 ; multiplicand
           0A6 : 0000;  -- m16sB: DW 0 ; multipler
           0A7 : 0000;  -- m16sc: DW 0 ; carry
           0A8 : 0000;  -- mcnt16s: DW 0 ; counter
           0A9 : 0000;  -- mres16sL: DW 0 ; result low
           0AA : 0000;  -- mres16sH: DW 0 ; result high
           0AB : B800;  -- 	LOADI  0
           0AC : 10E3;  -- 	STORE  dres16sR     ; clear remainder result
           0AD : 10E0;  -- 	STORE  d16sC1       ; clear carry
           0AE : 08DC;  -- 	LOAD   d16sN
           0AF : 58DD;  -- 	XOR    d16sD
           0B0 : 10DE;  -- 	STORE  d16sS        ; sign determination = N XOR D
           0B1 : B811;  -- 	LOADI  17
           0B2 : 10DF;  -- 	STORE  d16sT        ; preload counter with 17 (16+1)
           0B3 : 08DD;  -- 	LOAD   d16sD
           0B4 : 8019;  -- 	CALL   Abs          ; take absolute value of denominator
           0B5 : 10DD;  -- 	STORE  d16sD
           0B6 : 08DC;  -- 	LOAD   d16sN
           0B7 : 8019;  -- 	CALL   Abs          ; take absolute value of numerator
           0B8 : 10DC;  -- 	STORE  d16sN
           0B9 : 08DC;  -- 	LOAD   d16sN
           0BA : 601F;  -- 	SHIFT  -15          ; get msb
           0BB : 491C;  -- 	AND    One          ; only msb (because shift is arithmetic)
           0BC : 10E1;  -- 	STORE  d16sC2       ; store as carry
           0BD : 08DC;  -- 	LOAD   d16sN
           0BE : 6001;  -- 	SHIFT  1            ; shift <<1
           0BF : 50E0;  -- 	OR     d16sC1       ; with carry
           0C0 : 10DC;  -- 	STORE  d16sN
           0C1 : 08DF;  -- 	LOAD   d16sT
           0C2 : 6FFF;  -- 	ADDI   -1           ; decrement counter
           0C3 : 40D3;  -- 	JZERO  Div16s_sign  ; if finished looping, finalize result
           0C4 : 10DF;  -- 	STORE  d16sT
           0C5 : 08E3;  -- 	LOAD   dres16sR
           0C6 : 6001;  -- 	SHIFT  1            ; shift remainder
           0C7 : 50E1;  -- 	OR     d16sC2       ; with carry from other shift
