           02D : 8800;  -- 	RETURN
           02E : 087D;  -- 	LOAD   AtanY        ; Swap input arguments
           02F : 107F;  -- 	STORE  AtanT
           030 : 087C;  -- 	LOAD   AtanX
           031 : 107D;  -- 	STORE  AtanY
           032 : 087F;  -- 	LOAD   AtanT
           033 : 107C;  -- 	STORE  AtanX
