           00D : 9815;  -- 	OUT		SRAM_ADDR_JNEG
           00E : 9012;  -- 	IN		SRAM_DATA
           00F : B802;  -- 	LOADI	2
           010 : 9814;  -- 	OUT		SRAM_ADDR_JPOS
           011 : 9011;  -- 	IN 		SRAM_LOWER_ADDR
